{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true
  },
  "plugins": [
    "react",
    "import"
  ],
  "settings": {
    "import/resolve": {
      moduleDirectory: ["node_modules"]
    }
  },
  "globals": {
    "NODE_ENV": "development" 
  }
}
